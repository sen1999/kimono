<%= link_to "new", '/items/new' %><br>
<%= link_to "index", '/items' %>

<br><br>

<% @items.each do |item| %>
  <a href='/items/<%= item.id %>'><div>
  <%= image_tag item.image(:medium)%></div>
  <%= item.memo %></a>
  <%= render 'items/tag_list', tag_list: item.kind_list %>
  <%= render 'items/tag_list', tag_list: item.use_list %>
  <% end %>

<br><br>

